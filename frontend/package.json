{
  "name": "code",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-eslint": "6",
    "classnames": "^2.2.6",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "react-scripts": "1.1.4"
  },
  "dependencies": {
    "@material-ui/core": "^1.3.0",
    "@material-ui/icons": "^1.1.0",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "~4.2.2",
    "react-router-redux": "~5.0.0-alpha.9",
    "redux": "^3.6.0",
    "redux-form": "^6.5.0",
    "redux-saga": "^0.14.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "globals": {
      "localStorage": true,
      "document": true
    },
    "rules": {
      "semi": [
        "error",
        "never"
      ],
      "space-before-function-paren": [
        "error",
        "always"
      ],
      "jsx-quotes": [
        "error",
        "prefer-double"
      ],
      "no-unused-vars": [
        "error",
        {
          "varsIgnorePattern": "React"
        }
      ],
      "react/jsx-uses-vars": [
        2
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    }
  }
}
